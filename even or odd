// wap to check for even or odd
import java.util.*;
public class EvenOdd{
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
System.out.println("enter a no:");
int no=sc.nextInt();
if(no%2==0){
System.out.println("the no is even");
}
else{
System.out.println("the no is even");
}
}
}
